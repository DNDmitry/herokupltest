<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Building_number</fullName>
    <active>true</active>
    <errorConditionFormula>IF(REGEX(Street__c, &quot;.*[0-9]&quot;), false, true)</errorConditionFormula>
    <errorDisplayField>Street__c</errorDisplayField>
    <errorMessage>Street must contain a building number.</errorMessage>
</ValidationRule>
